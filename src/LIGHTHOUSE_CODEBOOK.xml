<Command
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www-01.ibm.com/software/analytics/spss/xml/extension-1.0.xsd"
  Name="LIGHTHOUSE CODEBOOK"
  Language="R">

  <Subcommand Name="" IsArbitrary="False" Occurrence="Optional">
    <Parameter Name="OUTFILE" ParameterType="OutputFile"/>
  </Subcommand>

  <Subcommand Name="DATA" IsArbitrary="False" Occurrence="Optional">
    <Parameter Name="NAME" ParameterType="QuotedString"/>
    <Parameter Name="FILE" ParameterType="InputFile"/>
  </Subcommand>

  <Subcommand Name="BY" IsArbitrary="True" Occurrence="Optional"/>

  <Subcommand Name="SPLITLABELS" IsArbitrary="True" Occurrence="Optional"/>

  <Subcommand Name="OPTIONS" IsArbitrary="False" Occurrence="Optional">
    <Parameter Name="OPEN" ParameterType="Keyword">
      <EnumValue Name="YES"/>
      <EnumValue Name="NO"/>
    </Parameter>
    <Parameter Name="HYPERLINKS" ParameterType="Keyword">
      <EnumValue Name="YES"/>
      <EnumValue Name="NO"/>
    </Parameter>
    <Parameter Name="DETAILMISSING" ParameterType="Keyword">
      <EnumValue Name="IFANY"/>
      <EnumValue Name="YES"/>
      <EnumValue Name="NO"/>
    </Parameter>
    <Parameter Name="NTEXTVALS" ParameterType="Integer"/>
    <Parameter Name="OVERWRITE" ParameterType="Keyword">
      <EnumValue Name="YES"/>
      <EnumValue Name="NO"/>
    </Parameter>
  </Subcommand>

  <Subcommand Name="INSTALL" Occurrence="Optional"/>

</Command>
